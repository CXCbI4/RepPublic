<Window x:Class="WpfApp2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="173*"/>
        </Grid.RowDefinitions>
        <Frame Content="" Margin="0,0,0,0" Name="MainFrame" Grid.Row="1"/>
        <Image Margin="682,15,15,15" Source="logo.png" Stretch="None" Grid.Row="0"/>
        <Button x:Name="zk_btn" Content="ЖК" HorizontalAlignment="Left" Margin="15,15,0,15" Width="75" Background="#FF04A0FF" Foreground="#FF37474F" FontFamily="Comic Sans MS" Height="36" Padding="10,10,10,10" Click="zk_btn_Click"/>
        <Button x:Name="doma_btn" Content="Дома" HorizontalAlignment="Left" Margin="95,15,0,15" Width="75" Background="#FF04A0FF" Foreground="#FF37474F" FontFamily="Comic Sans MS"  Height="36" Padding="10,10,10,10" Click="doma_btn_Click"/>

    </Grid>
</Window>
